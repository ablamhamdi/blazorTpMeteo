@inject IJSRuntime js


<div class="alert alert-info my-2">

 <button @onclick="SwapTheme" class="btn btn-primary"
>@SetThemeName Mode</button>
    @*Icon="@IconName.InvertColors"*@
</div>

@code {
     bool isDark;
     string SetThemeName => isDark ? "Dark" : "Light";
     async Task SwapTheme()
     {
             isDark = !isDark;
           await js.InvokeVoidAsync("themeChooser.setTheme", SetThemeName);
     }
}
